{
	"manifest_version": 3,
	"name": "Rebug Extension",
	"version": "1.0.0",
	"description": "A Chrome extension using Svelte",
	"permissions": ["activeTab", "scripting", "storage"],
	"host_permissions": ["<all_urls>"],
	"icons": {
		"128": "icons/favicon.png"
	},
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"128": "icons/favicon.png"
		}
	}
}
